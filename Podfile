# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'FLEX' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FLEX

  # swift initializer
  pod 'Then'

  # Rx
  pod 'R.swift'
 
  # swift auto layout
  pod 'SnapKit'

  # Flipboard Explorer is a set of in-app debugging and exploration tools for iOS 
  pod 'FLEX'

  # Routing
  pod 'RxFlow'

  # DI
  pod 'Pure'

  # firebase
  pod 'Firebase/Core'

  # Reactor
  pod 'ReactorKit'

  # layout
  pod 'Neon'

  # Rx
  pod 'RxSwift'
  pod 'RxCocoa'
  
  

end
